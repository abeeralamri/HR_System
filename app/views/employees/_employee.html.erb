<div id="<%= dom_id employee %>">
  <p>
    <strong>Name:</strong>
    <%= employee.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= employee.email %>
  </p>

  <p>
    <strong>Job:</strong>
    <%= employee.job %>
  </p>

  <p>
    <strong>Salary:</strong>
    <%= employee.salary %>
  </p>

  <p>
    <strong>E team:</strong>
    <%= employee.e_team %>
  </p>

  <p>
    <strong>Employee status:</strong>
    <%= employee.employee_status %>
  </p>

</div>
